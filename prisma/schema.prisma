datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Room {
  id       Int       @id @default(autoincrement())
  name     String
  capacity Int
  bookings Booking[]
}

model Booking {
  id        Int      @id @default(autoincrement())
  roomId    Int
  hour      Int
  date      DateTime
  name      String
  room      Room     @relation(fields: [roomId], references: [id])
}
